{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "functions": {
    "dist/index.cjs": {
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.cjs"
    },
    {
      "src": "/ws",
      "dest": "/dist/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
